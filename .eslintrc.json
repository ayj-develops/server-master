{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off"
    },
    "overrides": [
        {
            "files": [
                "__tests__/*.test.js"
            ]
        }
    ],
    "plugins": [
        "jest"
    ]
}